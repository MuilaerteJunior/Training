<Window x:Class="GSAP_Login.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GSAP_Login"
        mc:Ignorable="d"
        Title="MainWindow" Height="244" Width="461">
    <Grid Margin="0,0,2,-21">

        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="23" Margin="124,54,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="305"/>
        <Label x:Name="lblUser" Content="Username" HorizontalAlignment="Left" Margin="43,50,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblPassword" Content="Password" HorizontalAlignment="Left" Margin="43,92,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="txtPassword" HorizontalAlignment="Left" Margin="124,92,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="305"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="197,134,0,0" VerticalAlignment="Top" Width="75" Click="btnLogin_Click"/>
    </Grid>
</Window>
